

<form action="/accounts" method="POST" data-turbo="false" class="gap-5">
  <%= csrf_meta_tags %>
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <div class="flex flex-col gap-5 items-center justify-center">
    <label class="text-4xl" for="country">Country</label>
    <select
      id="country"
      name="account[country]"
      class="mt-2 block w-1/2 rounded-md border-0 py-1.5 pl-3 pr-10 text-gray-900"
    >
      <option value="US">United States</option>
      <option value="CA">Canada</option>
      <option value="MX">Mexico</option>
      <option value="IE">Ireland</option>

    </select>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <button type="submit" class="rounded-md w- bg-indigo-600 py-1.5 px-2.5 text-sm font-semibold text-white shadow-sm hover:" >
    Create a Stripe Account
  </button>
  </div>
  
</form>
